{
  "name": "core-js",
  "version": "4.0.0",
  "type": "commonjs",
  "scripts": {
    "postinstall": "node postinstall.js",
    "test": "node --test",
    "build": "node build.mjs",
    "build:docs": "vitepress build docs --outDir _site && typedoc --out _site/reference",
    "docs:dev": "vitepress dev docs",
    "format": "prettier -w .",
    "lint": "tsc --noEmit && eslint src"
  },
  "devDependencies": {
    "@types/node": "^20.8.6",
    "eslint": "^8.51.0",
    "eslint-plugin-es5": "^1.5.0",
    "execa": "^8.0.1",
    "prettier": "^3.0.3",
    "typedoc": "^0.25.2",
    "typedoc-plugin-mdn-links": "^3.1.0",
    "typescript": "^5.2.2",
    "vitepress": "^1.0.0-rc.22",
    "vitest": "^0.34.6",
    "vue": "^3.3.4"
  },
  "eslintConfig": {
    "plugins": [
      "es5"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:es5/no-es2015"
    ],
    "env": {
      "commonjs": true
    }
  },
  "dependencies": {
    "dom-parser": "^0.1.6"
  }
}
