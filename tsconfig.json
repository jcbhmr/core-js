{
  "compilerOptions": {
    "noEmit": true,
    "allowJs": true,
    "checkJs": true,
    "target": "ES5",
    "lib": [],
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "isolatedModules": true,
    "strict": false,
    "skipLibCheck": true,
    "strictBindCallApply": true
  },
  "exclude": [
    "node_modules",
    "out",
    "_site",
    "docs",
    ".github",
    "**/*.test.js",
    "**/*.test.mjs",
    "**/*.test.cjs",
    "**/*.test.ts",
    "**/*.test.mts",
    "**/*.test.cts"
  ],
  "typedocOptions": {
    "out": "out",
    "name": "core-js Reference",
    "readme": "none",
    "entryPoints": ["./*/polyfill.d.ts"],
    "exclude": [
      "postinstall.js",
      "eslint.config.js",
      "**/*.test.{js,mjs,cjs,ts,mts,cts}",
      "node_modules/**",
      "out/**",
      "_site/**",
      "docs/**",
      ".github/**",
      "lib/**"
    ],
    "skipErrorChecking": true,
    "plugin": [
      "typedoc-plugin-mdn-links",
      "typedoc-plugin-extras",
      "typedoc-plugin-merge-modules"
    ],
    "mergeModulesMergeMode": "module",
    "navigationLinks": {
      "Guide": "https://corejs.dev/core-js/guide/",
      "Reference": "https://corejs.dev/core-js/reference/",
      "GitHub": "https://github.com/corejs-dev/core-js"
    },
    "favicon": "https://fav.farm/ðŸŸ¨"
  }
}
