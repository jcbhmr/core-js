{
  "compilerOptions": {
    "noEmit": true,
    "allowJs": true,
    "checkJs": true,
    "target": "ESNext",
    "module": "NodeNext",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "NodeNext",
    "isolatedModules": true,
    "strict": false,
    "skipLibCheck": true
  },
  "exclude": ["docs", ".github", "node_modules"],
  "typedocOptions": {
    "out": "out",
    "entryPointStrategy": "expand",
    "entryPoints": ["."],
    "exclude": ["docs/**", ".github/**", "node_modules/**", "lib/**"],
    "skipErrorChecking": true,
    "plugin": ["typedoc-plugin-mdn-links", "typedoc-theme-hierarchy", "./my-typedoc-plugin.mjs"],
    "theme": "hierarchy",
    "navigationLinks": {
      "Guide": "https://corejs.dev/core-js/guide/",
      "Reference": "https://corejs.dev/core-js/reference/",
      "GitHub": "https://github.com/corejs-dev/core-js"
    }
  }
}
