{
  "compilerOptions": {
    "noEmit": true,
    "allowJs": true,
    "checkJs": true,
    "target": "ESNext",
    "lib": [],
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "isolatedModules": true,
    "strict": false,
    "skipLibCheck": true,
    "strictBindCallApply": true
  },
  "exclude": ["node_modules", "out", "_site", "docs", ".github"],
  "typedocOptions": {
    "out": "out",
    "name": "core-js Reference",
    "readme": "./README.md",
    "entryPoints": ["*/ponyfill.js"],
    "exclude": [
      "postinstall.js",
      "eslint.config.js",
      "my-typedoc-plugin.mjs",
      "**/*.mjs",
      "**/*.test.{js,mjs,cjs}",
      "node_modules/**",
      "out/**",
      "_site/**",
      "docs/**",
      ".github/**"
    ],
    "skipErrorChecking": true,
    "plugin": [
      "./my-typedoc-plugin.mjs",
      "typedoc-plugin-remove-empty-namespaces",
      "typedoc-plugin-merge-modules",
      "typedoc-plugin-not-exported",
      "typedoc-plugin-mdn-links"
    ],
    "navigationLinks": {
      "Guide": "https://corejs.dev/core-js/guide/",
      "Reference": "https://corejs.dev/core-js/reference/",
      "GitHub": "https://github.com/corejs-dev/core-js"
    }
  }
}
