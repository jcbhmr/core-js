{
  "compilerOptions": {
    "noEmit": true,
    "allowJs": true,
    "checkJs": true,
    "target": "ES5",
    "lib": [],
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "isolatedModules": true,
    "strict": false,
    "skipLibCheck": true,
    "strictBindCallApply": true
  },
  "exclude": [
    "node_modules",
    "docs",
    "**/*.test.js",
    "**/*.test.mjs",
    "**/*.test.cjs",
    "**/*.test.ts",
    "**/*.test.mts",
    "**/*.test.cts"
  ],
  "typedocOptions": {
    "name": "core-js Reference",
    "readme": "none",
    "entryPoints": ["./*/auto.d.ts", "./*/implementation.js"],
    "skipErrorChecking": true,
    "plugin": [
      "typedoc-plugin-mdn-links",
      "typedoc-plugin-extras",
      "typedoc-plugin-merge-modules",
      "typedoc-plugin-rename-defaults"
    ],
    "mergeModulesMergeMode": "module",
    "navigationLinks": {
      "Guide": "https://corejs.dev/core-js/guide/",
      "Reference": "https://corejs.dev/core-js/reference/",
      "GitHub": "https://github.com/corejs-dev/core-js"
    },
    "favicon": "https://avatars.githubusercontent.com/u/148142330?s=200&v=4"
  }
}
